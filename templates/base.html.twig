                                            </li>
                                            {% else %}
                                            <li class="aList me-sm-2">
                                                <p> {{ app.user.username }} </p>
                                                <p>Mon compte</p>
                                                <a class="my-2 my-sm-0" href="{{path('security_logout')}}">Déconnexion</a>
                                            </li>
                                           {% endif %}
                                        </ul>
                                    </dd>
                                </dl>
                            </div>
                        </div>
                    </div>
