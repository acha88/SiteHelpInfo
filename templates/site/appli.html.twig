{% extends "base.html.twig" %}

{% block body %}
    <div class="outWave">
        <div class="content">
            <h3 id="solution">Solutions GET-MS</h3>
        </div>
        <!--partie solution GET-MS-->
        <!--articles des solutions en BDD-->
        <section id="i" class="articles">
            {% for article in article %}
                <article id="{{article.id}}">
                    <div class="divRow">
                        <img class="imgArticle" src="{{ asset(article.image) }}" alt="image">
                        <div class="metadata">
                            <h4>{{ article.titlearticle }}</h4>
                            <div class="contentArticle">{{ article.contenu | raw }}</div>
                        </div>
                    </div>
                    <br>
                </article>
                <hr class="hrArticle">
            {% endfor %}
        </section>
    </div>
{% endblock body %}